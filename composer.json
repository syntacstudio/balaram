{
    "name": "syntac/balaram",
    "description": "Backup Laravel app to Telegram automatically",
    "license": "MIT",
    "authors": [
        {
            "name": "Willy Arisky",
            "email": "willy@syntac.co.id",
            "homepage": "https://syntac.co"
        }
    ],
    "homepage": "https://github.com/syntacstudio/backuplaraveltotelegram",
    "keywords": ["Laravel", "balaram", "Telegram", "Backup"],
    "require": {
        "php": "^7.0",
        "illuminate/support": "~5",
        "spatie/db-dumper": "^2.14",
        "longman/telegram-bot": "^0.60.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Syntac\\Balaram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syntac\\Balaram\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Syntac\\Balaram\\BalaramServiceProvider"
            ]
        }
    }
}
